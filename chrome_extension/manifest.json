{
  "manifest_version": 3,
  "name": "QuizMe Reset",
  "version": "1.0.2",
  "description": "An extension to reset your QuizMe progress",
  "icons": {
    "16": "images/reset16.png",
    "48": "images/reset48.png",
    "128": "images/reset128.png"
  },
  "action": {
    "default_popup": "src/index.html",
    "default_popup_height": 700,
    "default_popup_width": 700
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["scripting", "activeTab","storage"]
}
