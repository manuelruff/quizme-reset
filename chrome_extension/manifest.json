{
  "manifest_version": 3,
  "name": "QuizMe Reset",
  "version": "1.0.0",
  "description": "A basic extension to reset your QuizMe progress",
  "icons": {
    "32": "images/reset32p.png"
  },
  "action": {
    "default_popup": "src/index.html",
    "default_popup_height": 700,
    "default_popup_width": 700
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["scripting", "activeTab","tabs","storage"],
  "host_permissions": [
    "https://quizeme.co.il/*"
  ]
}
